[
  {
    "$schema": "https://json.schemastore.org/swcrc",
    "test": [".*.(js|ts)$"],
    "module": {
      "type": "commonjs",
      "ignoreDynamic": true
    },
    "jsc": {
      "target": "es2020",
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": false,
        "dynamicImport": false
      }
    }
  }
]

